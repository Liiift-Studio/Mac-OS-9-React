{
	"name": "@liiift-studio/mac-os9-ui",
	"version": "0.1.1",
	"description": "A pixel-perfect Mac OS 9 UI component library for React and TypeScript",
	"private": false,
	"sideEffects": false,
	"type": "module",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/cjs/index.js"
			}
		},
		"./styles": {
			"import": "./dist/esm/styles/theme.css",
			"require": "./dist/cjs/styles/theme.css"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE",
		"ATTRIBUTION.md"
	],
	"scripts": {
		"dev": "storybook dev -p 6006",
		"build": "tsup",
		"build:storybook": "storybook build",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "eslint src --ext .ts,.tsx",
		"lint:fix": "eslint src --ext .ts,.tsx --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
		"typecheck": "tsc --noEmit",
		"changeset": "changeset",
		"version": "changeset version",
		"release": "npm run build && changeset publish",
		"prepublishOnly": "npm run build"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.10",
		"@chromatic-com/storybook": "^3.2.2",
		"@storybook/addon-essentials": "^8.5.0",
		"@storybook/addon-interactions": "^8.5.0",
		"@storybook/addon-links": "^8.5.0",
		"@storybook/blocks": "^8.5.0",
		"@storybook/react": "^8.5.0",
		"@storybook/react-vite": "^8.5.0",
		"@storybook/test": "^8.5.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.1.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/react": "^18.3.18",
		"@types/react-dom": "^18.3.5",
		"@typescript-eslint/eslint-plugin": "^8.20.0",
		"@typescript-eslint/parser": "^8.20.0",
		"@vitejs/plugin-react": "^4.3.4",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.37.2",
		"eslint-plugin-react-hooks": "^5.1.0",
		"eslint-plugin-storybook": "^0.11.1",
		"jsdom": "^25.0.1",
		"prettier": "^3.4.2",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"storybook": "^8.5.0",
		"tsup": "^8.3.5",
		"typescript": "^5.7.3",
		"vite": "^6.0.7",
		"vitest": "^2.1.8"
	},
	"keywords": [
		"react",
		"typescript",
		"ui",
		"components",
		"mac-os-9",
		"retro",
		"design-system"
	],
	"author": "Liiift Studio",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Liiift-Studio/Mac-OS-9-React.git"
	},
	"homepage": "https://liiift-studio.github.io/Mac-OS-9-React/",
	"bugs": {
		"url": "https://github.com/Liiift-Studio/Mac-OS-9-React/issues"
	},
	"attribution": {
		"designs": "Mac OS 9 UI Kit by Michael Feeney (https://swallowmygraphicdesign.com/project/macostalgia)",
		"license": "CC BY 4.0 (https://creativecommons.org/licenses/by/4.0/)"
	}
}
